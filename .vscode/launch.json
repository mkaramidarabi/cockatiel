{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/server.js",
			"preLaunchTask": "npm: Start",
			"env": {
				"APP_MODE": "LOCAL",
				"FORCE_COLOR": "1"
			}
    }
  ]
}